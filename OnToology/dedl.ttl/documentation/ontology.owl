    <!-- https://w3id.org/def/dedl#accessControlModel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#accessControlModel">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Policy"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#AccessControlModel"/>
        <rdfs:comment xml:lang="en">Associates a Policy with its AccessControlModel to define and enforce resource access rules.</rdfs:comment>
        <rdfs:label xml:lang="en">accessControlModel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authenticationFactor -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authenticationFactor">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Authenticator"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#AuthenticationFactor"/>
        <rdfs:comment xml:lang="en">Indicates the AuthenticationFactor that an Authenticator uses.</rdfs:comment>
        <rdfs:label xml:lang="en">authenticationFactor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authenticationProcess -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authenticationProcess">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Authenticator"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#AuthenticationProcess"/>
        <rdfs:comment xml:lang="en">Links an Authenticator to the AuthenticationProcess used for identity verification.</rdfs:comment>
        <rdfs:label xml:lang="en">authenticationProcess</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authenticationProtocol -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authenticationProtocol">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Authenticator"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#AuthenticationProtocol"/>
        <rdfs:comment xml:lang="en">Specifies the AuthenticationProtocol an Authenticator follows.</rdfs:comment>
        <rdfs:label xml:lang="en">authenticationProtocol</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authenticationRiskContext -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authenticationRiskContext">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Authenticator"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#AuthenticationRiskContext"/>
        <rdfs:comment xml:lang="en">Associates an Authenticator with contextual factors that influence authentication risk.</rdfs:comment>
        <rdfs:label xml:lang="en">authenticationRiskContext</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authenticator -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authenticator">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Participant"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#Authenticator"/>
        <rdfs:comment xml:lang="en">Relates a Participant to an Authenticator performing authentication on their behalf.</rdfs:comment>
        <rdfs:label xml:lang="en">authenticator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authorizationArchitecture -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authorizationArchitecture">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Authorizator"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#AuthorizationArchitecture"/>
        <rdfs:comment xml:lang="en">Specifies the AuthorizationArchitecture used by an Authorizator.</rdfs:comment>
        <rdfs:label xml:lang="en">authorizationArchitecture</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authorizationProcess -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authorizationProcess">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Authorizator"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#AuthorizationProcess"/>
        <rdfs:comment xml:lang="en">Indicates the AuthorizationProcess followed by an Authorizator.</rdfs:comment>
        <rdfs:label xml:lang="en">authorizationProcess</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#authorizator -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#authorizator">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Participant"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#Authorizator"/>
        <rdfs:comment xml:lang="en">Declares a Participant to be an Authorizator responsible for enforcing authorization decisions.</rdfs:comment>
        <rdfs:label xml:lang="en">authorizator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#entity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#entity">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Participant"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#Entity"/>
        <rdfs:comment xml:lang="en">Associates a Participant with a particular Entity in the data ecosystem.</rdfs:comment>
        <rdfs:label xml:lang="en">entity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#identity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#identity">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/didl#Identity"/>
        <rdfs:comment xml:lang="en">Indicates the Identity belonging to an Entity.</rdfs:comment>
        <rdfs:label xml:lang="en">identity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#participant -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#participant">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#DataEcosystem"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#Participant"/>
        <rdfs:comment xml:lang="en">Specifies a Participant within a DataEcosystem.</rdfs:comment>
        <rdfs:label xml:lang="en">participant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#policy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#policy">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#Policy"/>
        <rdfs:comment xml:lang="en">Links an Entity to a Policy governing its usage or access rights.</rdfs:comment>
        <rdfs:label xml:lang="en">policy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#policyLanguage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#policyLanguage">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#Policy"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#PolicyLanguage"/>
        <rdfs:comment xml:lang="en">Specifies the PolicyLanguage used to express or encode a Policy.</rdfs:comment>
        <rdfs:label xml:lang="en">policyLanguage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dedl#process -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/dedl#process">
        <rdfs:domain rdf:resource="https://w3id.org/def/dedl#DataEcosystem"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dedl#Process"/>
        <rdfs:comment xml:lang="en">Associates a DataEcosystem with a particular Process.</rdfs:comment>
        <rdfs:label xml:lang="en">process</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/def/dedl#AccessControlModel -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#AccessControlModel">
        <rdfs:comment xml:lang="en">A conceptual framework defining how access to resources is managed, granted, and restricted.</rdfs:comment>
        <rdfs:label xml:lang="en">AccessControlModel</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#AuthenticationFactor -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#AuthenticationFactor">
        <rdfs:comment xml:lang="en">An element used to verify the identity of a user or entity (e.g. password, token, biometrics).</rdfs:comment>
        <rdfs:label xml:lang="en">AuthenticationFactor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#AuthenticationProcess -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#AuthenticationProcess">
        <rdfs:comment xml:lang="en">A procedure or sequence of steps that verify the identity of a user or entity.</rdfs:comment>
        <rdfs:label xml:lang="en">AuthenticationProcess</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#AuthenticationProtocol -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#AuthenticationProtocol">
        <rdfs:comment xml:lang="en">A specification of the interactions or steps used to perform authentication.</rdfs:comment>
        <rdfs:label xml:lang="en">AuthenticationProtocol</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#AuthenticationRiskContext -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#AuthenticationRiskContext">
        <rdfs:comment xml:lang="en">Contextual attributes (time, location, device) that affect authentication risk levels.</rdfs:comment>
        <rdfs:label xml:lang="en">AuthenticationRiskContext</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Authenticator -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Authenticator">
        <rdfs:comment xml:lang="en">An actor or component responsible for carrying out the authentication process.</rdfs:comment>
        <rdfs:label xml:lang="en">Authenticator</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#AuthorizationArchitecture -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#AuthorizationArchitecture">
        <rdfs:comment xml:lang="en">A structural framework that defines how authorization decisions are made and enforced.</rdfs:comment>
        <rdfs:label xml:lang="en">AuthorizationArchitecture</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#AuthorizationProcess -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#AuthorizationProcess">
        <rdfs:comment xml:lang="en">A method or set of steps determining if an entity may access a resource.</rdfs:comment>
        <rdfs:label xml:lang="en">AuthorizationProcess</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Authorizator -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Authorizator">
        <rdfs:comment xml:lang="en">An actor or component that assesses permissions and enforces authorization decisions.</rdfs:comment>
        <rdfs:label xml:lang="en">Authorizator</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#DataEcosystem -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#DataEcosystem">
        <rdfs:comment xml:lang="en">A socio-technical system in which participants share and manage data and associated processes.</rdfs:comment>
        <rdfs:label xml:lang="en">DataEcosystem</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Decision -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Decision">
        <rdfs:comment xml:lang="en">An outcome regarding an authorization request, e.g. Allow or Deny.</rdfs:comment>
        <rdfs:label xml:lang="en">Decision</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Entity -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Entity">
        <rdfs:comment xml:lang="en">Any identifiable object of a participant within the data ecosystem, e.g. a device or digital resource.</rdfs:comment>
        <rdfs:label xml:lang="en">Entity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Individual -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Individual">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/dedl#Participant"/>
        <rdfs:comment xml:lang="en">A subclass of Participant representing a legal person.</rdfs:comment>
        <rdfs:label xml:lang="en">Individual</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Organization -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Organization">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/dedl#Participant"/>
        <rdfs:comment xml:lang="en">A subclass of Participant representing a legal entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Organization</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Participant -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Participant">
        <rdfs:comment xml:lang="en">a stakeholder within the data ecosystem (either an Individual or Organization).</rdfs:comment>
        <rdfs:label xml:lang="en">Participant</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Policy -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Policy">
        <rdfs:comment xml:lang="en">A set of rules or guidelines that govern data usage, sharing, and access.</rdfs:comment>
        <rdfs:label xml:lang="en">Policy</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#PolicyLanguage -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#PolicyLanguage">
        <rdfs:comment xml:lang="en">A formal language for defining and expressing policies.</rdfs:comment>
        <rdfs:label xml:lang="en">PolicyLanguage</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/dedl#Process -->


    <owl:Class rdf:about="https://w3id.org/def/dedl#Process">
        <rdfs:comment xml:lang="en">A series of operations or activities performed within the data ecosystem.</rdfs:comment>
        <rdfs:label xml:lang="en">Process</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/didl#Identity -->


    <owl:Class rdf:about="https://w3id.org/def/didl#Identity">
        <rdfs:comment xml:lang="en">A unique collection of attributes, credentials, or characteristics of an entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Identity</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/def/dedl#ABAC -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#ABAC">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AccessControlModel"/>
        <rdfs:comment xml:lang="en">Attribute-based Access Control model, relying on attributes to define permissions.</rdfs:comment>
        <rdfs:label xml:lang="en">ABAC</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#ACP -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#ACP">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#PolicyLanguage"/>
        <rdfs:comment xml:lang="en">Access control policy language (ACP), a particular set of grammar, syntax rules (logical and mathematical), and operators which provides an abstraction-layer for access control policy specifications, see https://solid.github.io/authorization-panel/acp-specification/</rdfs:comment>
        <rdfs:label xml:lang="en">ACP</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Allow -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Allow">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#Decision"/>
        <rdfs:comment xml:lang="en">A Decision indicating that access is permitted.</rdfs:comment>
        <rdfs:label xml:lang="en">Allow</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Behavior -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Behavior">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AuthenticationRiskContext"/>
        <rdfs:comment xml:lang="en">A context factor reflecting user behavioral patterns (e.g., keystroke dynamics).</rdfs:comment>
        <rdfs:label xml:lang="en">Behavior</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#CapBAC -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#CapBAC">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AccessControlModel"/>
        <rdfs:comment xml:lang="en">Capability-based Access Control relying on tokens or &apos;capabilities&apos; to grant permissions.</rdfs:comment>
        <rdfs:label xml:lang="en">CapBAC</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Cryptography -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Cryptography">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AuthenticationProtocol"/>
        <rdfs:comment xml:lang="en">An authentication protocol leveraging cryptographic techniques to prove identity.</rdfs:comment>
        <rdfs:label xml:lang="en">Cryptography</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#DAC -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#DAC">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AccessControlModel"/>
        <rdfs:comment xml:lang="en">Discretionary Access Control, where resource owners grant or deny permissions.</rdfs:comment>
        <rdfs:label xml:lang="en">DAC</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Deny -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Deny">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#Decision"/>
        <rdfs:comment xml:lang="en">A Decision indicating that access is refused.</rdfs:comment>
        <rdfs:label xml:lang="en">Deny</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Device -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Device">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AuthenticationRiskContext"/>
        <rdfs:comment xml:lang="en">A factor in the authentication risk context indicating the device used for login.</rdfs:comment>
        <rdfs:label xml:lang="en">Device</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Exchange -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Exchange">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#Process"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Find -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Find">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#Process"/>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Geolocation -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Geolocation">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AuthenticationRiskContext"/>
        <rdfs:comment xml:lang="en">A factor in the authentication risk context representing physical or network location.</rdfs:comment>
        <rdfs:label xml:lang="en">Geolocation</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#HybridArchitecture -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#HybridArchitecture">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AuthorizationArchitecture"/>
        <rdfs:comment xml:lang="en">An AuthorizationArchitecture combining policy-based and token-based architecture</rdfs:comment>
        <rdfs:label xml:lang="en">HybridArchitecture</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#Kerberos -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#Kerberos">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AuthenticationProtocol"/>
        <rdfs:comment xml:lang="en">A protocol using a trusted third-party ticket-granting service for authentication.</rdfs:comment>
        <rdfs:label xml:lang="en">Kerberos</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#LDAP -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#LDAP">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AuthenticationProtocol"/>
        <rdfs:comment xml:lang="en">Lightweight Directory Access Protocol, often used to manage authentication data in directories.</rdfs:comment>
        <rdfs:label xml:lang="en">LDAP</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#MAC -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#MAC">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#AccessControlModel"/>
        <rdfs:comment xml:lang="en">Mandatory Access Control, where policies are enforced by a central authority.</rdfs:comment>
        <rdfs:label xml:lang="en">MAC</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/def/dedl#MPEG-21 -->


    <owl:NamedIndividual rdf:about="https://w3id.org/def/dedl#MPEG-21">
        <rdf:type rdf:resource="https://w3id.org/def/dedl#PolicyLanguage"/>
        <rdfs:comment xml:lang="en">A digital rights management and usage rights framework which can serve as a policy language.</rdfs:comment>
        <rdfs:label xml:lang="en">MPEG-21</rdfs:label>
